<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script
      type="text/javasript"
      src="//dapi.kakao.com/v2/maps/sdk.js?appkey=1380f11d7fd0867b6353a8b48a5aa7f6"
    ></script>
    <title>School</title>
  </head>
  <body>
    <div id="map" style="width: 100%; height: 350px"></div>
    <script>
      // 	fdadc7ae7f717f0f74286d7b010b44ee
      // 1380f11d7fd0867b6353a8b48a5aa7f6
      var mapConatiner = document.getElementById("map"),
        mapOption = {
          center: new kakao.maps.LatLng(33.450701, 126.570667),
          level: 3,
        };
      var map = new kakao.maps.Map(mapConatiner, mapOption);

      var markerPosition = new kakao.maps.LatLng(33.450701, 126.570667);

      var marker = new kakao.maps.Marker({
        position: markerPosition,
      });
      marker.setMap(map);
    </script>
  </body>
</html>
